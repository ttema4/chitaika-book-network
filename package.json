    "globals": "^17.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
